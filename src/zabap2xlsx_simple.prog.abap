             min          TYPE string,
             max          TYPE string,
             width        TYPE float,
             customwidth  TYPE string,
             style        TYPE string,
             bestfit      TYPE string,
             collapsed    TYPE string,
             hidden       TYPE string,
             outlinelevel TYPE string,
           END OF lty_column.

